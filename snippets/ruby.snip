snippet class
alias cla
	class ${1:`helper#filename_camelized()`}${2}
		${3}
	end
snippet defi
	def initialize${1}
		${2}
	end
snippet defp
	def ${1:name}${2}
		${4}
	end
	${3:private} :$1
snippet def
	def ${1:name}
		${2}
	end
snippet do
	do
		${1}
	end
snippet dop
	do |${1:param}|
		${2}
	end
snippet enc
options head
	# encoding: UTF-8
snippet module
alias mod
	module ${1:`helper#filename_camelized()`}
		${2}
	end
snippet hr
	puts "${1:*}"*80${2}
